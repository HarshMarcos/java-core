package com.learning.core.java.day3session2;

import java.util.Map;
import java.util.TreeMap;

public class D03P08e {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		TreeMap<Car, String> carDetails = new TreeMap<>();
		
		Car car1 = new Car("Bugati", 80050.0);
		Car car2 = new Car("Swift", 40050.0);
		Car car3 = new Car("Audi", 90050.0);
		Car car4 = new Car("Benz", 80050.0);
		
		carDetails.put(car1, "Details of car");
		carDetails.put(car2, "Details of car");
		carDetails.put(car3, "Details of car");
		carDetails.put(car4, "Details of car");
		
		for(Map.Entry<Car, String> entry : carDetails.entrySet()) {
			System.out.println(entry.getKey());
		}
	}
 
}
